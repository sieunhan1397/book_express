<h2>Search Books</h2>
<form action="/books" method="get">
  <div>
    <label for="title">Title</label>
    <input
      type="text"
      name="title"
      id="title"
      value="<%= searchOptions.title %>"
    />
  </div>
  <div>
    <label for="publishAfter">Published After</label>
    <input
      type="date"
      name="publishAfter"
      id="publishAfter"
      value="<%= searchOptions.publishAfter %>"
    />
  </div>
  <div>
    <label for="publishBefore">Published Before</label>
    <input
      type="date"
      name="publishBefore"
      id="publishBefore"
      value="<%= searchOptions.publishBefore %>"
    />
  </div>
  <button class="btn" type="submit">Search</button>
</form>
<br />
<div>
  <% books.forEach(book => { %>
  <img src="<%= book.coverImagePath %> " height="150" width="100" />
  <% }); %>
</div>
